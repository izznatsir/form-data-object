{
	"name": "form-data-object",
	"version": "0.1.0",
	"files": [
		"dist",
		"src"
	],
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"default": "./dist/index.js",
			"import": "./dist/index.js"
		}
	},
	"engines": {
		"node": ">=20"
	},
	"packageManager": "yarn@4.0.1",
	"type": "module",
	"scripts": {
		"build": "tsup",
		"format": "prettier -uw .",
		"test": "vitest run",
		"test:coverage": "vitest run --coverage"
	},
	"devDependencies": {
		"@vitest/coverage-istanbul": "^1.0.0-beta.5",
		"lint-staged": "^15.1.0",
		"prettier": "^3.1.0",
		"tsup": "^8.0.1",
		"typescript": "^5.3.2",
		"vite-tsconfig-paths": "^4.2.1",
		"vitest": "^1.0.0-beta.5"
	},
	"prettier": {
		"overrides": [
			{
				"files": [
					"*.yaml",
					"*.yml"
				],
				"options": {
					"tabWidth": 2
				}
			}
		],
		"printWidth": 80,
		"tabWidth": 4,
		"trailingComma": "es5",
		"useTabs": true
	},
	"lint-staged": {
		"*": "prettier -uw"
	}
}
